pipeline{
    agent any
        environment{
            PATH="/opt/apache-maven-3.9.3/bin:$PATH"
        }
        stages{
            stage("git checkout"){
                steps{
                    git 'https://github.com/saikirangoud203/myweb'
                }
            }
            stage("maven build"){
              steps {
                 sh "mvn clean package"
              }          
            }
        }
}
